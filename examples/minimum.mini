def fib(n: int): int = {
  if n < 2 {
    n
  } else {
    fib(n-2) + fib(n-1)
  }
}

print(fib(1));
print(fib(2));
print(fib(3));
print(fib(4));
print(fib(5));
print(fib(6));
print(fib(7));
print(fib(8));

let last_sum_result: int = 0

def sum(n: int): int = {
  let acc = 0;
  while n > 0 {
    acc = acc + n
    n = n - 1
  }
  last_sum_result = acc
  acc
}

print(sum(10))
print(last_sum_result)
